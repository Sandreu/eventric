{
  "name": "{{ doc.codeName | prepareForJSON }}",
  "module": "{{doc.module}}",
  "source": "{{ doc.outputPath }}",
  "description": "{{doc.description | prepareForJSON}}",
  {% if doc.params %}
  "params": [
    {% for param in doc.params %}
      {
        "name": "{{param.name}}",
        "description": "{{param.description | prepareForJSON}}",
        "typeList": "{{param.typeList}}"
      }{% if loop.last != true %},{% endif %}
    {% endfor %}
  ],
  {% endif %}
  "returns": {
    "typeList": "{{doc.returns.typeList}}",
    "description": "{{doc.returns.description | prepareForJSON}}"
  },
  "example": "{{doc.example}}"
}